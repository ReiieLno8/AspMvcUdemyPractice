<div class="container">
    <div class="row pt-4 pb-4">
        <div class="col-6">
            <h2 class="text-primary">
                Company List
            </h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Company" asp-action="CompanyUpsert" class="btn btn-outline-primary">
                <i class="bi bi-house-door"></i> Create
            </a>
            <a asp-area="" asp-controller="" asp-action="" class="btn btn-primary">
                <i class="bi bi-house-door"></i> Home
            </a>
        </div>
    </div>
    <table id="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    StreetAddress
                </th>
                <th>
                    City
                </th>
                <th>
                    State
                </th>
                <th>
                    Postal
                </th>
                <th>
                    PhoneNumber
                </th>
                <th>

                </th>
            </tr>
        </thead>
    </table>
</div>
@section Scripts{
    <script src="~/js/company.js"></script>
}